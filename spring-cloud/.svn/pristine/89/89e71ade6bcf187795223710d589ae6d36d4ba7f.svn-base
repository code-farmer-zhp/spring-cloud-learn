    public void setIsGroup(Integer isGroup) {
        this.isGroup = isGroup;
    }

    public BigDecimal getFavoritePrice() {
        return favoritePrice;
    }

    public void setFavoritePrice(BigDecimal favoritePrice) {
        this.favoritePrice = favoritePrice;
    }

    public String getFavoriteChannel() {
        return favoriteChannel;
    }

    public void setFavoriteChannel(String favoriteChannel) {
        this.favoriteChannel = favoriteChannel;
    }

    public String getSource() {
        return source;
    }

    public void setSource(String source) {
        this.source = source;
    }

    public String getIsMobilePrice() {
        return isMobilePrice;
    }

    public void setIsMobilePrice(String isMobilePrice) {
        this.isMobilePrice = isMobilePrice;
    }

    public Map<String, Object> getActivitys() {
        return activitys;
    }

    public void setActivitys(Map<String, Object> activitys) {
        this.activitys = activitys;
    }

