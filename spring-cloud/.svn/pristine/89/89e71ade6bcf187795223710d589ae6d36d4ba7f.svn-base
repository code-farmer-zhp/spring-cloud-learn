        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getIsSpecial() {
        return isSpecial;
    }

    public void setIsSpecial(String isSpecial) {
        this.isSpecial = isSpecial;
    }

    public String getRlink() {
        return rlink;
    }

    public void setRlink(String rlink) {
        this.rlink = rlink;
    }

    public String getIsFlowers() {
        return isFlowers;
    }

    public void setIsFlowers(String isFlowers) {
        this.isFlowers = isFlowers;
    }

