    /**
     * 是否是手机专享价  1 是，0 不是
     */
    private String isMobilePrice;
    /**
     * 行销活动数据
     */
    private Map<String, Object> activitys;
    
    /**
     * 类目Id
     */
    private String cpSeq;
    /**
     * 自营商品状态
     */
    private String status;
    /**
     * 是否促销
     */
    private String isSpecial;
    /**
     * 来源链接
     */
    private String rlink;
    /**
     * 是否花呗
     */
    private String isFlowers;
    /**
     * 是否花呗
     */
    private List<String> tags;
    /**
     * 价格差(收藏价-卖场价)
     */
    private Integer priceDifference;
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getMember_guid() {
        return member_guid;
    }

    public void setMember_guid(String member_guid) {
        this.member_guid = member_guid;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getIt_pic() {
        return it_pic;
    }

    public void setIt_pic(String it_pic) {
        this.it_pic = it_pic;
    }

    public String getSource_url() {
        return source_url;
    }

    public void setSource_url(String source_url) {
        this.source_url = source_url;
    }

    public String getSell_no() {
        return sell_no;
    }

    public void setSell_no(String sell_no) {
